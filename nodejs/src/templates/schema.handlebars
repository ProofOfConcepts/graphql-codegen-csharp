using System;
using System.Collections.Generic;
using Newtonsoft.Json;
using Newtonsoft.Json.Converters;
using Agoda.CodeGen.GraphQL;

#region input types

namespace Generated
{

    // scalars if used 
{{#each (getValueTypeIfUsed scalars)}}
    {{~> structs }}

{{/each}}

    // types if used 
{{#each operations }}   
  {{#each (getTypeIfUsed innerModels ../types)}}
    {{~> classes }}

  {{/each}}
{{/each}}

    // input types
{{#each (getInputTypeIfUsed inputTypes operations) }}   
  {{~> classes }}

{{/each}}

    // enums if used 
{{#each (getEnumTypesIfUsed inputTypes operations types enums)}}
  {{~> enumTemplate }}

{{/each}}

}

#endregion 

